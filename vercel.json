{
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "outputDirectory": "dist",
    "framework": "vite",
    "functions": {
        "api/**/*.js": {
            "includeFiles": "node_modules/@google/genai/**"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ]
}